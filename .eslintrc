{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "react-hooks"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "$ExtractArgument": true,
    "$ExtractComponentProps": true,
    "$ExtractObject": true,
    "$ExtractReturn": true,
    "$Keys": true,
    "$PropertyType": true,
    "$Shape": true,
    "$Values": true,
    "$ExtractActionParams": true,
    "Activity": true,
    "Answer": true,
    "AnswerValue": true,
    "AnyUser": true,
    "AuditItem": true,
    "AvailabilityType": true,
    "BasicQuotation": true,
    "BatchItem": true,
    "BidAnswers": true,
    "BroadcastUserWithDate": true,
    "BuyOperationStatus": true,
    "CategoryType": true,
    "Choice": true,
    "ChoiceOf": true,
    "Company": true,
    "Conversation": true,
    "CostPriceParameters": true,
    "CostPriceParametersForm": true,
    "Currency": true,
    "Data": true,
    "DetailedStatus": true,
    "EventType": true,
    "ExchangeRates": true,
    "ExternalUser": true,
    "FixedQuotation": true,
    "FormQuestion": true,
    "Generator": true,
    "Goal": true,
    "Grid": true,
    "GridType": true,
    "GridWithIsAvailableOffline": true,
    "Image": true,
    "IncotermDict": true,
    "Instance": true,
    "Language": true,
    "LanguageDict": true,
    "LanguageKey": true,
    "LinkedOperationQuestionToPrefillKey": true,
    "Loading": true,
    "LogisticsType": true,
    "LogisticCost": true,
    "MapOfAnswers": true,
    "MapOfAudits": true,
    "MapOf": true,
    "MapOfBoolean": true,
    "MapOfBroadcastUsersWithDates": true,
    "MapOfChoices": true,
    "MapOfCompanies": true,
    "MapOfConversationMessages": true,
    "MapOfConversations": true,
    "MapOfCostPriceParameters": true,
    "MapOfDates": true,
    "MapOfGrids": true,
    "MapOfImages": true,
    "MapOfInstances": true,
    "MapOfOffers": true,
    "MapOfOperations": true,
    "MapOfQuestions": true,
    "MapOfQuotations": true,
    "MapOfSites": true,
    "MapOfStatuses": true,
    "MapOfTransactions": true,
    "MapOfUsers": true,
    "MercurialIndexedQuotation": true,
    "Message": true,
    "MessageStatus": true,
    "MessageType": true,
    "ModalInfomation": true,
    "NonMobileUser": true,
    "Offer": true,
    "OfferStatus": true,
    "OfferType": true,
    "OfferWithBidderWithSite": true,
    "OfferWithQuality": true,
    "OfferWithSite": true,
    "OnboardingTrackName": true,
    "OnboardingTrackStep": true,
    "Operation": true,
    "OperationDirection": true,
    "OperationFiltersType": true,
    "OperationTableType": true,
    "OperationTag": true,
    "OperationType": true,
    "PercentageIndexedQuotation": true,
    "PhoneContact": true,
    "QuantityFieldType": true,
    "Question": true,
    "Quotation": true,
    "quotationGridId": true,
    "RawPhoneContact": true,
    "RequestItem": true,
    "Role": true,
    "Router": true,
    "SavedQuestions": true,
    "SectionItemType": true,
    "SellOperationStatus": true,
    "Settings": true,
    "Site": true,
    "SiteWithCompany": true,
    "SpecificQuestionKey": true,
    "SpotQuotation": true,
    "State": true,
    "Status": true,
    "SuggestedProfitableBidAnswers": true,
    "SuggestedProfitableBid": true,
    "Suggestion": true,
    "SyntheticInputEvent": true,
    "T": true,
    "TableDataType": true,
    "TableItemField": true,
    "Transaction": true,
    "TransactionFilterType": true,
    "TransactionWithSiteWithCompany": true,
    "Todo": true,
    "TodoType": true,
    "Ui": true,
    "User": true,
    "UserWithConversation": true,
    "UserWithSiteWithCompany": true,
    "Volume": true,
    "WebUser": true
  },
  "rules": {
    "object-curly-newline": "warn",
    "jsx-quotes": "warn",
    "no-undef": "warn",
    "no-unused-vars": "warn",
    "react/no-unused-prop-types": "warn",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "arrow-parens": [
      "error",
      "always"
    ],
    "arrow-body-style": [
      2,
      "as-needed"
    ],
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "no-magic-numbers": [2, { "ignore": [0, 1] }],
    "react/jsx-no-literals": "warn",
    "import/imports-first": 0,
    "import/newline-after-import": 0,
    "import/no-dynamic-require": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-named-as-default": 0,
    "import/no-unresolved": 0,
    "import/extensions": [
      "off",
      "never"
    ],
    "import/prefer-default-export": 0,
    "no-restricted-imports": [
      "error",
      {
        "patterns": [
          "../*",
          "./*"
        ]
      }
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "jsx-a11y/aria-props": 2,
    "jsx-a11y/heading-has-content": 0,
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/label-has-for": 2,
    "jsx-a11y/mouse-events-have-key-events": 2,
    "jsx-a11y/role-has-required-aria-props": 2,
    "jsx-a11y/role-supports-aria-props": 2,
    "max-len": ["error", { "code": 120 }],
    "newline-per-chained-call": 0,
    "no-confusing-arrow": 0,
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "no-named-as-default-member": 0,
    "prefer-template": 2,
    "class-methods-use-this": 0,
    "react/forbid-prop-types": 2,
    "react/prop-types": 0,
    "react/jsx-first-prop-new-line": [
      2,
      "multiline"
    ],
    "react/jsx-filename-extension": 0,
    "react/jsx-no-target-blank": 0,
    "react/require-extension": 0,
    "require-yield": 0,
    "import/no-webpack-loader-syntax": 0,
    "react/prefer-stateless-function": 2,
    "sort-keys": "warn",
    "sort-imports": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["./"]
      }
    }
  }
}
